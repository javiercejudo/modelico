{
  "name": "modelico",
  "version": "21.2.1",
  "description": "Universal library for serialisable immutable models",
  "main": "dist/modelico.js",
  "jsnext:main": "dist/modelico.es2015.js",
  "scripts": {
    "docs-prepare": "gitbook install",
    "docs-watch": "npm run docs-prepare && gitbook serve",
    "t": "gulp test",
    "bt": "npm run build && npm run t",
    "test": "npm run standard && flow && npm run t",
    "test-dev": "npm run build-dev && npm run build-browser-spec && gulp test",
    "test-browsers": "./scripts/browser_tester.sh",
    "send-coverage": "./scripts/coverage_reporter.sh",
    "standard": "standard --fix \"src/**/*.js\" \"test/**/*.js\"",
    "prebuild": "npm run standard && flow && rm -rf dist && mkdir dist",
    "build": "npm run build-src && npm run build-browser-spec",
    "build-src": "npm run build-src-dev && npm run build-src-min",
    "build-src-dev": "rollup -c && rollup -c rollup.es2015.config.js",
    "build-src-min": "uglifyjs dist/modelico.js -c -m > dist/modelico.min.js && babili dist/modelico.es2015.js > dist/modelico.es2015.min.js",
    "build-browser-spec": "rollup -c ./test/rollup.config.js",
    "watch": "rollup -c -w",
    "watch-spec": "rollup  -c ./test/rollup.config.js -w",
    "version": "npm run build && git add -A dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/javiercejudo/modelico.git"
  },
  "keywords": [
    "model",
    "immutable",
    "reviver",
    "objects",
    "serialization",
    "json",
    "stringify",
    "parse"
  ],
  "author": "Javier Cejudo <javier@javiercejudo.com> (http://www.javiercejudo.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/javiercejudo/modelico/issues"
  },
  "homepage": "https://github.com/javiercejudo/modelico#readme",
  "devDependencies": {
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babili": "0.0.11",
    "flow-bin": "^0.38.0",
    "foreman": "^2.0.0",
    "gitbook-cli": "^2.3.0",
    "gulp": "^3.9.0",
    "gulp-codecov": "^2.0.2",
    "gulp-istanbul": "^1.0.0",
    "gulp-mocha": "^4.0.1",
    "gulp-plumber": "^1.1.0",
    "karma": "^1.1.2",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sauce-launcher": "^1.0.0",
    "mocha": "^3.2.0",
    "phantomjs-prebuilt": "^2.1.14",
    "rimraf": "^2.5.4",
    "rollup": "^0.41.1",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-json": "^2.0.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-watch": "^3.1.0",
    "should": "^11.1.2",
    "standard": "^8.6.0",
    "uglify-js": "^2.7.5"
  },
  "dependencies": {
    "ajv": "^4.11.2"
  }
}
