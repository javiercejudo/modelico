{
  "name": "modelico",
  "version": "12.4.0",
  "description": "Universal library for serialisable immutable models",
  "main": "dist/modelico.js",
  "scripts": {
    "test": "npm run build && gulp test",
    "test-dev": "npm run build-dev-es5 && npm run build-browser-spec-es5 && gulp test",
    "test-browsers": "./scripts/browser_tester.sh",
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "npm run build-dev && npm run build-min && npm run build-browser-spec",
    "build-dev": "npm run build-dev-es5 && npm run build-dev-es2015",
    "build-dev-es5": "rollup -c",
    "build-dev-es2015": "rollup -c rollup.es2015.config.js",
    "build-min": "npm run build-min-es5 && npm run build-min-es2015",
    "build-min-es5": "uglifyjs dist/modelico.js -c -m > dist/modelico.min.js",
    "build-min-es2015": "uglifyjs dist/modelico.es2015.js -c -m > dist/modelico.es2015.min.js",
    "build-browser-spec": "npm run build-browser-spec-es5 && npm run build-browser-spec-es2015",
    "build-browser-spec-es5": "rollup -c ./test/rollup.config.js",
    "build-browser-spec-es2015": "rollup ./test/modelicoSpec.js -f umd -n modelicoSpec -o dist/modelico-spec.es2015.js",
    "version": "npm run build && git add -A dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/javiercejudo/modelico.git"
  },
  "keywords": [
    "model",
    "immutable",
    "reviver",
    "objects",
    "serialization",
    "json",
    "stringify",
    "parse"
  ],
  "author": "Javier Cejudo <javier@javiercejudo.com> (http://www.javiercejudo.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/javiercejudo/modelico/issues"
  },
  "homepage": "https://github.com/javiercejudo/modelico#readme",
  "devDependencies": {
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015-rollup": "^1.1.1",
    "gulp": "^3.9.0",
    "gulp-codecov": "^2.0.1",
    "gulp-coverage": "^0.3.38",
    "gulp-istanbul": "^0.10.2",
    "gulp-mocha": "^2.1.3",
    "gulp-plumber": "^1.1.0",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^1.0.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sauce-launcher": "^0.3.1",
    "phantomjs-prebuilt": "^2.1.7",
    "rimraf": "^2.4.3",
    "rollup": "^0.26.0",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-json": "^2.0.0",
    "should": "^8.3.0",
    "uglify-js": "git://github.com/mishoo/UglifyJS2.git#8571a08a930474b68cb0e2daf3c7e666dcdcb15e"
  }
}
