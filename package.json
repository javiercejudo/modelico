{
  "name": "modelico",
  "version": "15.0.0",
  "description": "Universal library for serialisable immutable models",
  "main": "dist/modelico.js",
  "jsnext:main": "dist/modelico.es2015.js",
  "scripts": {
    "test": "flow && npm run build && gulp test",
    "test-dev": "npm run build-dev && npm run build-browser-spec && gulp test",
    "test-browsers": "./scripts/browser_tester.sh",
    "send-coverage": "./scripts/coverage_reporter.sh",
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "npm run build-dev && npm run build-min && npm run build-browser-spec",
    "build-dev": "rollup -c && rollup -c rollup.es2015.config.js",
    "build-min": "uglifyjs dist/modelico.js -c -m > dist/modelico.min.js",
    "build-browser-spec": "rollup -c ./test/rollup.config.js",
    "version": "npm run build && git add -A dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/javiercejudo/modelico.git"
  },
  "keywords": [
    "model",
    "immutable",
    "reviver",
    "objects",
    "serialization",
    "json",
    "stringify",
    "parse"
  ],
  "author": "Javier Cejudo <javier@javiercejudo.com> (http://www.javiercejudo.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/javiercejudo/modelico/issues"
  },
  "homepage": "https://github.com/javiercejudo/modelico#readme",
  "devDependencies": {
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "6.13.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "flow-bin": "^0.30.0",
    "foreman": "^1.4.1",
    "gulp": "^3.9.0",
    "gulp-codecov": "^2.0.2",
    "gulp-istanbul": "^1.0.0",
    "gulp-mocha": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "karma": "^1.1.2",
    "karma-chrome-launcher": "^1.0.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sauce-launcher": "^1.0.0",
    "phantomjs-prebuilt": "^2.1.10",
    "rimraf": "^2.5.4",
    "rollup": "^0.35.10",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-flow": "^1.0.1",
    "rollup-plugin-json": "^2.0.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "should": "^8.4.0",
    "uglify-js": "^2.7.0"
  }
}
